<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings> 
    <add key="AWSAccessKey" value="" />
    <add key="AWSSecretKey" value="" />
    <add key="AWSRegion" value="sa-east-1" />          
    <add key="S3TransferAccelerationEndPoint" value="{bucketName}.s3-accelerate.amazonaws.com"/>
    <add key="S3TransferAcceleration" value="false" /> 
    <add key="BucketName" value="" />
    <add key="KeyName" value="" />
    <add key="RootFolderName" value="" />
    <add key="RootListFolderName" value="" />
    <add key="CopyEmptyFolders" value="true" />
    <add key="RecycleBufferInMinutes" value="30" />
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
</configuration>